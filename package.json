{
  "name": "e-commerce-backend",
  "version": "1.0.0",
  "description": "production ready ecommerce backend api with automation testing with jest  and ci cd pipeline using github action abd devloy on aws",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gsubhampatra/e-commerce-backend.git"
  },
  "keywords": [
    "nodejs",
    "javascript",
    "jest",
    "automationtesting",
    "githubaction",
    "aws",
    "cicd"
  ],
  "author": "gsubhampatra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gsubhampatra/e-commerce-backend/issues"
  },
  "homepage": "https://github.com/gsubhampatra/e-commerce-backend#readme",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.6.1",
    "nodemailer": "^6.9.15",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
